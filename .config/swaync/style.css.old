    box-shadow: none;
    background: black;
    transition: all .15s ease-in-out;
    border: none
}

.widget-title {
    color: black;
    margin: 20px 20px 5px 20px;
}
.widget-title>button {
    color: black;
    background: black;
    border: 2px solid black;
    border-radius: 16px;
}
.widget-title>button:hover {
