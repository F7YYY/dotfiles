    padding: 8px;
    margin: 20px 10px 10px;
    border-radius: 16px;
    background: black;
}
.widget-buttons-grid>flowbox>flowboxchild>button {
    margin: 3px;
    background: black;
    border-radius: 16px;
    color: black
}
.widget-buttons-grid>flowbox>flowboxchild>button:hover {
    color: black;
}
.widget-menubar>box>.menu-button-bar>button {
    border: none;
    background: transparent
}
.widget-volume {
    background: black;
    padding: 8px;
    margin: 10px;
    border-radius: 16px;
    color: black
}
.widget-volume>box>button {
    background: transparent;
    border: none
}
.widget-backlight {
    background: black;
    padding: 8px;
    margin: 10px;
    border-radius: 16px;
    color: black
}
.widget-inhibitors {
    margin: 8px;
}
.widget-inhibitors>button {
    color: black;
    background: black;
    border: 2px solid black;
    box-shadow: none;
    border-radius: 16px
}
.widget-inhibitors>button:hover {
    background: black
}

