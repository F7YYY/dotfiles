/*─────────────────────────────(MODULES)───*/
.close-button {
  background-color: @WHITE;
  border-radius: 100%;
  border: 2px solid white;
  box-shadow: none;
  /*margin-right: 13px;
  margin-top: 0px;*/
  padding: 0px;
  /*min-height: 20px;
  min-width: 20px;*/
  text-shadow: none;
}
.close-button:hover {
  background-color: @RED;
}

/* Widget title styles */
.widget-title {
  padding: 0px;
}
.widget-title label {
  font-weight: bold;
  color: unset;
  margin-left: 0.941rem;
}
.widget-title button {
  border: unset;
  background-color: unset;
  border-radius: 1.159rem;
  -gtk-outline-radius: 1.159rem;
  padding: 0.141rem 0.141rem;
  margin-left: 2rem;
}
.widget-title button label {
  font-size: 1.0409rem;
  color: unset;
  margin-right: 0.841rem;
}
.widget-title button:hover {
  background-color: unset;
}
.widget-title button:active {
  background-color: unset;
}

/* Widget buttons styles */
.widget-buttons-grid {
  border-radius: 1.159rem;
  -gtk-outline-radius: 1.159rem;
  background-color: unset;
}
.widget-buttons-grid>flowbox>flowboxchild>button {
  border:none;
  background-color: unset;
  border-radius: 9999px;
  min-width: 4.522rem;
  min-height: 2.927rem;
  padding: unset;
  margin: unset;
}
.widget-buttons-grid>flowbox>flowboxchild>button label {
  font-size: 1.3027rem;
  color: unset;
}
.widget-buttons-grid>flowbox>flowboxchild>button:hover {
  background-color: unset;
}
.widget-buttons-grid>flowbox>flowboxchild>button:checked {
  background-color: unset;
  color: unset;
}

/* Widget volume styles */
.widget-volume {
  background-color: unset;
  padding: 8px;
  margin: 8px;
  border-radius: 1.159rem;
}
.widget-volume trough {
  background-color: unset;
}
.widget-volume trough slider {
  background-color: unset;
  border-radius: 100%;
  min-height: 1.25rem;
}

/* Widget mpris styles */
.widget-mpris {
  background-color: unset;
  padding: 8px;
  margin: 8px;  
  border-radius: 1.159rem;
}
.widget-mpris-player {
  padding: 8px;
  margin: 8px;
  color: unset;
}
.widget-mpris-title {
  font-weight: bold;
  font-size: 1.25rem;
}
.widget-mpris-subtitle {
  font-size: 1.1rem;
}

/* Widget dnd styles */
.widget-dnd label {
  font-weight: bold;
  font-size: 97%;
  color: unset;
  margin-left: 0.941rem;
}
.widget-dnd>switch {
  font-size: initial;
  border-radius: 12px;
  background: unset;
  border: 1px solid @WHITE;
  box-shadow: none;
  margin-right: 0.941rem;
}
.widget-dnd>switch:checked {
  background: unset;
}
.widget-dnd>switch slider {
  background: unset;
  border-radius: 12px;
}

/*─────────────────────────(AUTOMATIONS)───*/
